<?xml version="1.0" encoding="utf-8"?>
<styles  antiAlias="true">
	<rubric xpos="18" ypos="8" width="800">
		<image>assets/rubric.swf</image>
	</rubric>
	<images>
		<image item="background">assets/background.jpg</image>
		<image item="tickCross">assets/tickcross.swf</image>
	</images>
	<buttons>
		<button item="see_mistake" visible="false" enabled = "true" xpos="600" ypos="90" >
			<image>assets/button_see_mistake.swf</image>
			<mp3>../../audio/click_low.mp3</mp3>
		</button>
		<button item="check_answers" visible="false" enabled = "true" xpos="600" ypos="90">
			<image>assets/button_check_ans.swf</image>
			<mp3>../../audio/click_low.mp3</mp3>
		</button>
		<button item="next" visible="false" enabled = "true" xpos="600" ypos="90">
			<image>assets/button_next.swf</image>
			<mp3>../../audio/click_low.mp3</mp3>
		</button>
		<button item="score" visible="false" enabled = "true" xpos="600" ypos="90">
			<image>assets/button_score.swf</image>
			<mp3>../../audio/click_low.mp3</mp3>
		</button>
	</buttons>
	<sounds>
		<sound item="rightsound">
			<link>assets/correct.mp3</link>
		</sound>
		<sound item="wrongsound">
			<link>assets/wrong.mp3</link>
		</sound>
	</sounds>
	<assets>
		<asset item="text_Area" paragraphForm="false" highlightColour="0xCCFF99" backgroundColour="0xFFFFFF" leftEdge="18" rightEdge="700" topEdge="147" lineSpacing="14"/>
		<asset item="try_again_ani" xpos="50" ypos="95">
			<image>assets/try_again_ani.swf</image>
		</asset>
		<asset item="correctAnsBox" xpos="5" ypos="90">
			<image>assets/correctAnsBox.swf</image>
		</asset>
		<asset item="scoreBar" visible="true" xpos="18" ypos="450">
			<image>assets/template_scorebar.swf</image>
		</asset>
		<asset item="liveformat" font="verdana" size="14"  bold="true" color="000000"/>
		<asset item="greyformat" font="verdana" size="14" bold="true" color="CCCCCC"/>
		<asset item="greenformat" font="verdana" size="14" bold="true" color="009900"/>
		<asset item="greenformatwrong" font="verdana" size="14" bold="true" color="009900" underline="true"/>
	
	</assets>
</styles>