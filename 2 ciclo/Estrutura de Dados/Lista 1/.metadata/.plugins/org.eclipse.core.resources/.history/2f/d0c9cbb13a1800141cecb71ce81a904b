
public class Aula1 {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		String merda = "\n\n\nHello Word!\nporra";
		int i= 0;
			
		while(i <=3){
			System.out.println(i)
			i++;
		}
		
		for(i=0;i<=5;i++){
			System.out.print(merda);
		}
		
	}

}
